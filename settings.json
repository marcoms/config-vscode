{
	"workbench.colorTheme": "One Dark Pro",
	"window.menuBarVisibility": "hidden",
	"editor.fontSize": 16,
	"editor.fontFamily": "'Operator Mono Book', 'Office Code Pro', monospace",
	"editor.tabSize": 4,
	"editor.renderWhitespace": "boundary",
	"editor.cursorStyle": "line",
	"editor.multiCursorModifier": "ctrlCmd",
	"editor.insertSpaces": false,
	"editor.wordWrap": "on",
	"editor.scrollBeyondLastLine": false,
	"editor.parameterHints": true,
	"editor.autoClosingBrackets": false,
	"editor.formatOnType": false,
	"editor.formatOnPaste": true,
	"editor.autoIndent": true,
	"editor.acceptSuggestionOnEnter": "off",
	"editor.cursorBlinking": "blink",
	"editor.fontLigatures": true,
	"editor.renderControlCharacters": true,
	"editor.renderLineHighlight": "all",
	"editor.matchBrackets": true,
	"editor.useTabStops": true,
	"editor.trimAutoWhitespace": true,
	"diffEditor.renderSideBySide": true,
	"workbench.editor.showIcons": true,
	"workbench.editor.closeOnFileDelete": false,
	"workbench.tips.enabled": false,
	"workbench.startupEditor": "none",
	"workbench.iconTheme": "vs-seti",
	"window.openFilesInNewWindow": "off",
	"window.title": "${folderName}${separator}${activeEditorMedium}",
	"window.newWindowDimensions": "maximized",
	"files.encoding": "utf8",
	"files.eol": "\n",
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true
	},
	"files.trimTrailingWhitespace": true,
	"files.insertFinalNewline": true,
	"files.watcherExclude": {
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"**/node_modules/**": true,
		"build/**": true,
		"dist/**": true
	},
	"files.useExperimentalFileWatcher": true,
	"explorer.openEditors.visible": 0,
	"search.useRipgrep": true,
	"search.quickOpen.includeSymbols": true,
	"update.channel": "none",
	"html.format.indentInnerHtml": true,
	"html.format.indentHandlebars": true,
	"markdown.preview.fontSize": 16,
	"typescript.tsdk": "/home/marcoms/.config/yarn/global/node_modules/typescript/lib",
	"typescript.npm": "/usr/bin/npm",
	"terminal.external.linuxExec": "gnome-terminal",
	"terminal.integrated.shell.linux": "/usr/bin/fish",
	"terminal.integrated.fontLigatures": true,
	"terminal.integrated.cursorBlinking": true,
	"git.path": "/usr/bin/git",
	"rust.rustup": {
		"toolchain": "nightly-x86_64-unknown-linux-gnu",
		"nightlyToolchain": "nightly-x86_64-unknown-linux-gnu"
	},
	"rust.mode": "rls",
	"rust.rls": {
		"useRustfmt": false
	},
	"rust.rustLangSrcPath": "/home/marcoms/.rustup/toolchains/nightly-x86_64-unknown-linux-gnu/lib/rustlib/src/rust/src",
	"rust.cargoHomePath": "/home/marcoms/.cargo"
}
